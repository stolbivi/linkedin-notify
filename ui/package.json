{
  "name": "linkedin-manager-ui",
  "version": "0.0.3",
  "description": "UI for extension to monitor activity on your LinkedIn profile",
  "main": "dist/app.js",
  "scripts": {
    "init": "export SET NODE_OPTIONS=--openssl-legacy-provider",
    "cleanup": "node ./scripts/build.js --command init",
    "build-ui": "webpack --mode development --env env=dev",
    "release-ui": "webpack --mode development --env env=prod",
    "build": "npm run build-ui && tsc",
    "release": "npm run release-ui && tsc",
    "start": "npm run init && node .",
    "serve": "npm run init && npm run tsoa && nodemon src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.16.7",
    "@babel/plugin-transform-react-inline-elements": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "fs-extra": "^11.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.6",
    "minimist": "^1.2.7",
    "nodemon": "^2.0.20",
    "sass": "^1.47.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4",
    "url": "^0.11.0"
  }
}
