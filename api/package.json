{
  "name": "linkedin-manager-api",
  "version": "0.0.3",
  "description": "API for extension to monitor activity on your LinkedIn profile",
  "main": "dist/app.js",
  "scripts": {
    "init": "export SET NODE_OPTIONS=--openssl-legacy-provider",
    "cleanup": "node ./scripts/build.js --command init",
    "build-api": "npx tsoa spec-and-routes && tsc",
    "build-ui": "webpack --mode development",
    "build": "npm run cleanup && npm run build-api && npm run build-ui",
    "start": "npm run init && node .",
    "serve": "npm run init && npm run tsoa && nodemon src/app.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
